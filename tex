use volodymyrnehir;
show tables ;

select * from cars;
select * from cars where year >2000;
select * from cars where year < 2000;
select * from cars where year in (2008,2009,2010);
select * from cars where year != 2008 and year !=2009 and year !=2010 ;
select * from cars where year = price;
select * from cars where model = 'bmw' and year > 2014;
select * from cars where model = 'audi' and year <2014;
select * from cars where id limit 5;
select * from cars  order by id desc limit 5;
select sum(price) from cars where model ='kia';
select model, count(*)  from cars group by model  ;
select model,count(*) as maxModel from cars group by model order by  maxModel desc limit 1;
select * from cars where model like '_a%' and model like '%a_';
select model from cars where length(model) >8;
select * from cars where price > (select avg(price) from cars);
